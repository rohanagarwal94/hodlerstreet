<style>
    @media only screen and (min-width: 560px) {
        #Search {
            display: none;
        }
    }
</style>


<% if (theme.search.insight) { %>
    <div class="search-form" id="Search">
        <input type="text" class="ins-search-input search-form-input" placeholder="<%= __('index.search') %>" style="float:right;border-radius:5px;padding:9.682px 22.591px 9.682px 16.136px;margin-top:4px;margin-right:3px;"/>
    </div>
<% } else if (theme.search.swiftype) { %>
    <div class="search-form">
        <input type="text" class="st-default-search-input search-form-input" placeholder="<%= __('index.search') %>" />
    </div>
<% } else if (theme.search.baidu) { %>
    <%- partial('search/baidu') %>
<% } else { %>
    <%- search_form({text: __('index.search')}) %>
<% } %>